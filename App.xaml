<Application x:Class="calendar2.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:calendar2"
             xmlns:conv="clr-namespace:calendar2.Converters"
             StartupUri="MainWindow.xaml">
    
    <Application.Resources>
        <conv:ComboBoxItemToEnumConverter x:Key="DayOfWeekConverter"/>
        <conv:TimeSpanToStringConverter x:Key="TimeSpanConverter"/>
    </Application.Resources>

</Application>
